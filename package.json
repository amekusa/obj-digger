{
  "name": "obj-digger",
  "version": "2.2.0",
  "description": "Safely access the properties of deeply nested objects",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "npm-watch",
    "test": "mocha --enable-source-maps",
    "build": "rollup -c",
    "clean": "rm -rf ./dist",
    "prod": "npm run clean; NODE_ENV=production npm run build",
    "cov": "c8 mocha --enable-source-maps && c8 report -r lcov",
    "cov:pub": "codecovcli --verbose create-commit"
  },
  "watch": {
    "test": {
      "patterns": [
        "src",
        "test"
      ],
      "inherit": true
    },
    "build": {
      "patterns": "src",
      "inherit": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amekusa/obj-digger.git"
  },
  "keywords": [
    "util",
    "utility",
    "general-purpose",
    "object",
    "dig",
    "json"
  ],
  "author": "amekusa (amekusa.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amekusa/obj-digger/issues"
  },
  "homepage": "https://github.com/amekusa/obj-digger#readme"
}
